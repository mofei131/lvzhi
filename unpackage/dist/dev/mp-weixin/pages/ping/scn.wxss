
.flexbox{
	display: flex;
	width: 600rpx;
	align-items: center;
	justify-content: space-between;
	box-sizing: border-box;
}
.pickflex image{
	margin-left: 20rpx;
}
.pickflex{
	display: flex;
	align-items: center;
}
.listf image{
	width: 19rpx;
	height: 34rpx;
}
.listf view{
	color: #646464;
	font-size: 34rpx;
	font-weight: bold;
}
.listf{
	width: 680rpx;
	height: 100rpx;
	background-color: #EAEAEA;
	margin: auto;
	padding: 0 37rpx 0 41rpx;
	box-sizing: border-box;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-radius: 20rpx;
	margin-bottom: 30rpx;
}
.box{
	padding-top: 20rpx;
}

