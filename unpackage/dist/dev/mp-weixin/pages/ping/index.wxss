
.toptitle image{
	width: 40rpx;
	height: 40rpx;
}
.toptitle{
	width: 750rpx;
	margin: auto;
	display: flex;
	align-items: flex-end;
	padding-left: 20rpx;
	box-sizing: border-box;
	margin-bottom: 44rpx;
}
.tologin{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 99;
}
.listf image{
	width: 19rpx;
	height: 34rpx;
}
.listf view{
	color: #646464;
	font-size: 34rpx;
	font-weight: bold;
}
.listf{
	width: 680rpx;
	height: 100rpx;
	background-color: #EAEAEA;
	margin: auto;
	margin-top: 50rpx;
	padding: 0 37rpx 0 41rpx;
	box-sizing: border-box;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-radius: 20rpx;
}
.topimg{
	margin-bottom: 49rpx;
}

